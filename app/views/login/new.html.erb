

  <h1>Login</h1>
    <div>
      <div>
      <%= form_for user_sessions_path do |f| %>
      </div>
      <div>
        <%= f.label :email %><br/>
        <%= f.text_field :email %>
      </div>
      <div>
        <%= f.label :password %><br/>
        <%= f.text_field :password %>
      </div>
      <div>
        <%= f.submit 'Submit' %>
      </div>

      <% end %>
    </div>
